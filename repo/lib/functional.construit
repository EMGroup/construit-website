map is (f(x))(x in $1);
filter is (x if f(x) else @)(x in $1);

function foldr {
	local i;
	local tmp;
	tmp = $2;

	for (i=$1#; i>0; i--) {
		tmp = f($1[i],tmp);
	}

	return tmp;
}

function foldl {
	local i;
	local tmp;
	tmp = $2;

	for (i=1; i<=$1#; i++) {
		tmp = f(tmp,$1[i]);
	}

	return tmp;
}

